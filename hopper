-- ============================================================
-- INSTANT TELEPORT CHECK (0.1ms execution priority)
-- ============================================================
do
    local TeleportService = game:GetService("TeleportService")
    local Players = game:GetService("Players")
    
    -- CRITICAL: Set this to your actual Place IDs
    local NEWPLAYER_PLACE_ID = 0  -- REPLACE WITH YOUR NEW PLAYER PLACE ID
    local MAIN_PLACE_ID = 0       -- REPLACE WITH YOUR MAIN PLACE ID
    local BOT_NAME = "BRAINROT"
    
    -- Ultra-fast teleport setup
    pcall(function() TeleportService:SetTeleportGui(workspace) end)
    
    -- Minimal wait for game load
    if not game:IsLoaded() then 
        game.Loaded:Wait() 
    end
    
    -- Wait for LocalPlayer with timeout
    local waitStart = tick()
    repeat 
        task.wait() 
    until Players.LocalPlayer or (tick() - waitStart) > 5
    
    local LocalPlayer = Players.LocalPlayer
    
    -- INSTANT PLACE CHECK & TELEPORT
    if game.PlaceId == NEWPLAYER_PLACE_ID then
        print("[" .. BOT_NAME .. "] Wrong place detected - teleporting immediately...")
        pcall(function() TeleportService:SetTeleportGui(nil) end)
        pcall(function() TeleportService:TeleportCancel() end)
        
        -- Immediate teleport
        local success = pcall(function()
            TeleportService:Teleport(MAIN_PLACE_ID, LocalPlayer)
        end)
        
        if success then
            print("[" .. BOT_NAME .. "] Teleport initiated - script terminated")
        else
            warn("[" .. BOT_NAME .. "] Teleport failed - retrying...")
            task.wait(0.5)
            TeleportService:Teleport(MAIN_PLACE_ID, LocalPlayer)
        end
        
        -- Stop script execution here
        return
    end
    
    print("[" .. BOT_NAME .. "] Correct place - continuing with brainrot tracker...")
end

